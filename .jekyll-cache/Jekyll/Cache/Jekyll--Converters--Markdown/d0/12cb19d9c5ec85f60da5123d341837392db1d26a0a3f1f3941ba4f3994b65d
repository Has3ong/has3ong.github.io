    <span class="k">if</span> <span class="n">i</span> <span class="o">&gt;</span> <span class="mi">5000000</span> <span class="ow">and</span> <span class="n">i</span> <span class="o">==</span> <span class="mi">5000000</span><span class="p">:</span>
        <span class="k">continue</span>

<span class="k">print</span><span class="p">(</span><span class="s">"First time :"</span><span class="p">,</span> <span class="n">time</span><span class="o">.</span><span class="n">time</span><span class="p">()</span> <span class="o">-</span> <span class="n">start</span><span class="p">)</span>  <span class="c1"># 현재시각 - 시작시간 = 실행 시간
</span>
<span class="n">start</span> <span class="o">=</span> <span class="n">time</span><span class="o">.</span><span class="n">time</span><span class="p">()</span>  <span class="c1"># 시작 시간 저장
</span>
